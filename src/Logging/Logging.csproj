<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>Majenka.Logging</PackageId>
    <Company>Majenka</Company>
    <Authors>Majenka</Authors>
    <Version>1.0.1</Version>
    <PackageDescription>Logging library for dotnet core</PackageDescription>
    <RepositoryUrl>https://github.com/majenka/logging</RepositoryUrl>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyName>Majenka.Logging</AssemblyName>
    <RootNamespace>Majenka.Logging</RootNamespace>
    <Nullable>enable</Nullable>
    <WarningsAsErrors>nullable</WarningsAsErrors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="5.0.0" />
  </ItemGroup>

</Project>
