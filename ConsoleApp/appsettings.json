{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "File": {
      "Path": "/var/log/console-app.log",
      "BufferLines": 100,
      "MaxFileSize": 5242880,
      "MaxRetainedFiles": 5,
      "FlushInterval": 2500,
      "DateFormat": "yyyy-MM-dd HH:mm:ss.fff",
      "LogDate": true
    }
  }
}
